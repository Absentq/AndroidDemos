<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.lynxz.listdemos"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".GridViewBasicActivity"/>
        <activity android:name=".GridViewInListViewActivity"/>
        <activity android:name=".RecyclerViewDemo"/>
        <activity android:name=".RecycleViewAnimationActivity"/>
        <activity
            android:name=".LoadMoreRVActivity"
            android:label="@string/title_activity_load_more_rv"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".PopupMenuRvActivity"
            android:label="@string/title_activity_popup_menu_rv"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".RvHeaderActivity"
            android:label="@string/title_activity_rv_header"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".TimeLineActivity">
        </activity>
    </application>

</manifest>